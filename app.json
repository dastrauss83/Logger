{
  "expo": {
    "name": "Logger",
    "slug": "Logger",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./App/Assets/icon.png",
    "splash": {
      "image": "./App/Assets/poo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000033"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.dastrauss83.Logger"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./App/Assets/poo.png",
        "backgroundColor": "#000033"
      }
    },
    "web": {
      "favicon": "./App/Assets/poo.png"
    }
  }
}
